<?xml version="1.0" encoding="UTF-8"?>
<!-- https://cloud.google.com/appengine/docs/standard/java/config/cron -->
<cronentries>
    <cron>
        <url>/timeline?rt</url>
        <description>delete retweeted tweets below average</description>
        <schedule>every 180 minutes</schedule>
    </cron>
    <cron>
        <url>/timeline</url>
        <description>delete the rest</description>
        <schedule>every 2 hours</schedule>
    </cron>
</cronentries>