<?xml version="1.0" encoding="UTF-8"?>
<!-- https://cloud.google.com/appengine/docs/standard/java/config/cron -->
<cronentries>
    <cron>
        <url>/timeline?rt</url>
        <description>Delete retweeted tweets below average</description>
        <schedule>every 288 minutes</schedule>
    </cron>
    <cron>
        <url>/timeline?ot</url>
        <description>Delete old tweets</description>
        <schedule>every 467 minutes</schedule>
    </cron>
    <cron>
        <url>/timeline</url>
        <description>Delete recent tweets</description>
        <schedule>every 2 hours</schedule>
    </cron>
</cronentries>